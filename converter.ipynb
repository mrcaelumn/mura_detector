    "from PIL import Image\n",
    "import os\n",
    " \n",
    "Input_dir = 'mura_data/new_normal_images/'\n",
    "Out_dir = 'mura_data/new_grayscale_images/'\n",
    "a = os.listdir(Input_dir)\n",
    " \n",
    "for i in a:\n",
    "    print(i)\n",
    "    I = Image.open(Input_dir+i)\n",
    "    L = I.convert('L')\n",
    "    L.save(Out_dir+i)"
   ]
