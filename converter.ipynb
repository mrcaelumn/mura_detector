    "from PIL import Image\n",
    "import os\n",
    " \n",
    "Input_dir = 'rgb_defect/'\n",
    "Out_dir = 'grayscale_defect/'\n",
    "a = os.listdir(Input_dir)\n",
    "# convert into grayscale images\n",
    "for i in a:\n",
    "    print(i)\n",
    "    I = Image.open(Input_dir+i)\n",
    "    L = I.convert('L')\n",
    "    L.save(Out_dir+i)"
   ]
